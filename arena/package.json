{
  "name": "arena",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "match": "npm run build && node .dist/arena/src/cli.js match",
    "train": "npm run build && node .dist/arena/src/cli.js train",
    "train:composite": "npm run build && node .dist/arena/src/cli.js train-composite",
    "train:spawn": "npm run build && node .dist/arena/src/cli.js train-spawn",
    "replay": "npm run build && node .dist/arena/src/cli.js replay",
    "serve": "npm run build && node .dist/arena/src/server.js",
    "serve:grpc": "npm run build && node .dist/arena/src/cli.js serve-grpc"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "onnxruntime-web": "^1.24.1"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "typescript": "~5.9.3"
  }
}
